<!DOCTYPE html>
<html lang="en">
  <%- include ('./partials/head.ejs') %>
  <body>
    <header><%- include ('./partials/nav.ejs') %></header>
    <main>
      <h2>All Ress</h2>
      <% if (ress.length > 0) { %> <% ress.forEach(ress => { %>
        <article>
          <header>
            <h3><%= ress.title %></h3>
          </header>
          <p><%= ress.body %></p>
          <footer>Read More 👉🏾</footer>
        </article>
      <% }) %> <% } else { %>
      <h1>You have no Ress</h1>
      <% }%>
    </main>
    <%- include('./partials/footer.ejs') %>
  </body>
</html>
